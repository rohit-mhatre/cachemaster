{
  "name": "cache-layer-backend",
  "version": "1.0.0",
  "description": "High-performance in-memory cache layer with Redis-like features",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "benchmark": "npm run benchmark:get && npm run benchmark:set && npm run benchmark:mixed",
    "benchmark:get": "ts-node benchmarks/get-benchmark.ts",
    "benchmark:set": "ts-node benchmarks/set-benchmark.ts",
    "benchmark:mixed": "ts-node benchmarks/mixed-workload.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "cache",
    "redis",
    "in-memory",
    "lru",
    "lfu",
    "fifo"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.0",
    "joi": "^17.9.0",
    "winston": "^3.11.0",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.0",
    "dotenv": "^16.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/jest": "^29.5.0",
    "@types/supertest": "^6.0.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "supertest": "^6.3.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  }
}
